{
  "name": "emphasismine",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "func start",
    "test": "npx jest",
    "tdd": "npx jest --watchAll",
    "tcr": "npx nodemon -e js -x \"npx jest && git add -A && git commit -m 'wip'\"",
    "make": "node make.js"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "cheerio": "^1.2.0",
    "he": "^1.2.0",
    "node-fetch": "^2.6.1",
    "tumblr.js": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "dotenv": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.6.2"
  },
  "overrides": {
    "undici": "6.23.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "transform": {
      ".ts": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "esModuleInterop": true
        }
      }
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
