{
  "name": "emphasismine",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "func start",
    "test": "npx jest",
    "tdd": "npx jest --watchAll",
    "tcr": "npx nodemon -e js -x \"npx jest && git add -A && git commit -m 'wip'\"",
    "make": "node make.js"
  },
  "dependencies": {
    "@threadsjs/threads.js": "^1.9.0",
    "axios": "^1.6.7",
    "browser": "^0.2.6",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^2.6.1",
    "tumblr.js": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "transform": {
      ".ts": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "esModuleInterop": true
        }
      }
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
